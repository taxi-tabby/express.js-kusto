    "prisma:studio": "prisma studio --schema=src/app/db/schema.prisma",
    "prisma:reset": "prisma migrate reset --schema=src/app/db/schema.prisma",
    "prisma:push": "prisma db push --schema=src/app/db/schema.prisma"
  },
